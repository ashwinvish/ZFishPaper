\begin_body

\begin_layout Title
Structure and function of cells in a neural integrator
\end_layout

\begin_layout Author
Ashwin Vishwanathan,
\begin_inset Formula $^{1}$
\end_inset

 Kayvon Daie,
\begin_inset Formula $^{3}$
\end_inset

 Alex Ramirez,
\begin_inset Formula $^{3}$
\end_inset


\begin_inset Newline newline
\end_inset

 Jeff W Lichtman, 
\begin_inset Formula $^{4}$
\end_inset

Emre Aksay,
\begin_inset Formula $^{3}$
\end_inset

 and H.
 Sebastian Seung
\begin_inset Formula $^{1,2}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $^{1}$
\end_inset

Neuroscience Institute and 
\begin_inset Formula $^{2}$
\end_inset

Computer Science Department
\begin_inset Newline newline
\end_inset

Princeton University, Princeton, NJ 08544
